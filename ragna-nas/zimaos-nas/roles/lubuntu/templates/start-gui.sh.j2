#!/bin/bash
# Start Lubuntu GUI
# Generated by Ansible - RagnaNano Lab

echo "Starting Lubuntu Desktop Environment..."

# Check if X server is already running
if pgrep -x "Xorg" > /dev/null; then
    echo "GUI is already running."
    exit 0
fi

# Start the graphical target
sudo systemctl start graphical.target

echo "GUI started. Use 'sudo systemctl stop graphical.target' to stop."